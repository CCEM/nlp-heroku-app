{% extends "layout.jinja2" %}
{% block content %}
<h1>Subreddits we've evaluated:</h1>

<h1>5 Most Positive</h1>
<ul>
  {% for sub in positive %}
  <li>{{ sub[0] }}: {{ sub[1] }}</li>
  {% endfor %}
</ul>

<h1>5 Least Positive</h1>
<ul>
  {% for sub in negative %}
  <li>{{ sub[0] }}: {{ sub[1] }}</li>
  {% endfor %}
</ul>

<h1>5 Most Neutral</h1>
<ul>
  {% for sub in neutral%}
  <li>{{ sub[0] }}: {{ sub[1] }}</li>
  {% endfor %}
</ul>

{% endblock content %}
